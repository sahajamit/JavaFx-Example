<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.text.Text?>
<GridPane fx:id="gridPane" fx:controller="com.sahajamit.ChildController" xmlns:fx="http://javafx.com/fxml"
          alignment="BASELINE_CENTER"
          hgap="2" vgap="2">
    <Text fx:id="messageText" GridPane.rowIndex="0" GridPane.columnIndex="0" GridPane.columnSpan="2" />
    <Button fx:id="yesBtn" text="Yes" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
    <Button fx:id="noBtn" text="No" GridPane.rowIndex="1" GridPane.columnIndex="1" />
</GridPane>